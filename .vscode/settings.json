{
  "typescript.tsdk": "node_modules/typescript/lib",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**",
      "**/node_modules/**"
    ]
  },
  "cSpell.enabledFileTypes": {
    "*": true,
    "javascript": false
  },
  "nodejs-testing.extensions": [
    {
      "extensions": [
        "mts",
        "cts",
        "ts"
      ],
      "parameters": [
        "--import", "@septh/ts-run"
      ]
    }
  ]
}
